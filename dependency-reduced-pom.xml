<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>aws-gradious</groupId>
  <artifactId>CodeEvaluator</artifactId>
  <name>CodeEvaluator</name>
  <version>0.0.1-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.1.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration />
      </plugin>
    </plugins>
  </build>
  <properties>
    <jackson-databind.version>2.11.2</jackson-databind.version>
    <hibernate.version>5.4.21.Final</hibernate.version>
    <maven.compiler.target>1.8</maven.compiler.target>
    <json-simple.version>1.1.1</json-simple.version>
    <gson.version>2.8.2</gson.version>
    <maven.compiler.source>1.8</maven.compiler.source>
    <aws-java-sdk.version>1.11.241</aws-java-sdk.version>
    <aws-lambda-java-core.version>1.2.0</aws-lambda-java-core.version>
    <postgresql.version>42.2.16</postgresql.version>
    <aws-lambda-java-events.version>3.1.0</aws-lambda-java-events.version>
  </properties>
</project>
